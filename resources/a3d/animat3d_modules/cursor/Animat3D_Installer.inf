hand		= "A3D Handwriting.ani"
unavailiable	= "A3D Unavailable.ani"
vert		= "A3D Vertical Resize.ani"
horz		= "A3D Horizontal Resize.ani"
dgn1		= "A3D Diagonal Resize 1.ani"
dgn2		= "A3D Diagonal Resize 2.ani"
